<div class="container" id="glavna">
    <form class="form-inline" method="get" action="GetData">
        <div class="form-group">
            <label for="matBr">Unesi matične brojeve odvojene zarezom (npr. 12,150,304):   </label>
            <input class="form-control" type="text" id="matBr" name="matBr" required>
        </div>
        <input type="submit" id="btnTrazi" value="Traži" class="btn btn-primary" style="width:120px; margin-left:15px;">
    </form>

    <div id="bodi">
        @{
            if (Convert.ToInt32(ViewData["test"]) == 0)
            {
                <p></p>
            }
            else if (Convert.ToInt32(ViewData["test"]) == 1)
            {
                Html.RenderPartial("PartialViewDestimulacije");
            }
            else
            {

            }

        }
    </div>

</div>


